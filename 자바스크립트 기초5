var i = 0;
while(i < 10) {
    document.write("chocolate latte"+i+"<br />");
    i = i + 1
}


for(var i = 0; i < 10; i = i+1){
   document.write("maesil ice cream"+i+"<br />");
}
    

for(var i = 0; i < 10; i++){
   if (i===5){
     break;
   }
   document.write("maesil ice cream"+i+"<br />");
}

for(var i = 0; i < 10; i++){
  if (i===5){
      continue;
    }
    document.write("maesil ice cream"+i+"<br />");
}

