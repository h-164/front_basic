           function mybirthday() {
   		        document.write(823);
            }
            mybirthday();


            function number() {
                i=0;
                while (i<10){
                    document.write(i + "<br />");
                    i += 1 ;
                }
            }
            number();
            number();
            number();


            function favoritecolor() {
                return 'purple';
                return 'hot pink';
                return 'light yellow';
            }
            alert(favoritecolor());


            function argupara(arg){
                return arg*10;
            }
            alert(argupara(3));
        </script>
